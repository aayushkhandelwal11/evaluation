<div>
   <%= form_tag('/sessions/create') do %>
    <fieldset>
      <legend>Please Log In</legend>

      <table>
        <tr>
          <td><%= label_tag :email, 'Email' %></td>
          <td><%= text_field :email, params[:email] %></td>
        </tr>
        <tr>
          <td><%= label_tag :password, 'Password' %></td>
          <td><%= password_field_tag :password, params[:password] , size: 30%></td>
        </tr>

      </table>
      <%= submit_tag "Login" %>
      </div>
    </fieldset>
  <% end %>
</div>
<%= link_to 'new user', new_user_path %>